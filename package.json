{
  "name": "prep-chatbot",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "test": "jest --detectOpenHandles",
    "add": "pm2 start app/index --name \"prep-homol\"",
    "restart": "pm2 restart \"prep-homol\" --update-env",
    "logs": "pm2 logs \"prep-homol\" --lines 1000",
    "stop": "pm2 stop \"prep-homol\"",
    "remove": "pm2 remove \"prep-homol\"",
    "docker_restart": "docker rm -f prep-chatbot; bash build-container.sh; bash sample-run-container.sh",
    "docker_logs": "docker logs prep-chatbot",
    "webhook": "npx bottender messenger webhook set",
    "profileGet": "npx bottender messenger profile get",
    "profileSet": "npx bottender messenger profile set"
  },
  "dependencies": {
    "@sentry/node": "^5.15.4",
    "apiai-promise": "^1.0.0",
    "async-child-process": "^1.1.1",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bottender": "^1.4.10",
    "child_process": "^1.0.2",
    "cron": "^1.8.2",
    "dialogflow": "^1.2.0",
    "dotenv": "^4.0.0",
    "encodeurl": "^1.0.2",
    "event-stream": "^4.0.1",
    "express": "^4.17.1",
    "fs-extra": "^7.0.1",
    "get-audio-duration": "0.0.1",
    "grpc": "^1.24.2",
    "jsonfile": "^5.0.0",
    "messaging-api-messenger": "^0.7.16",
    "minimist": "^1.2.5",
    "moment": "^2.24.0",
    "node-google-calendar": "^1.1.1",
    "nodemailer": "^6.4.16",
    "nodemon": "^1.19.4",
    "promisepipe": "^3.0.0",
    "remove-accents": "^0.4.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "requisition": "^1.7.0",
    "util.promisify": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "jest": "^25.2.7"
  }
}
